{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "minItems": 1,
  "items": {
    "type": "object",
    "required": ["name", "type"],
    "properties": {
      "name": { "type": "string" },
      "address": { "type": "string" },
      "phone": { "type": "string" },
      "hours": { "type": "string" },
      "type": { "type": "string" },
      "lat": { "type": ["number","null"] },
      "lng": { "type": ["number","null"] },
      "description": { "type": "string" },
      "source": { "type": "string" },
      "verified_at": { "type": "string", "format":"date" },
      "status": { "type":"string" }
    },
    "additionalProperties": false
  }
}
